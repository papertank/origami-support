{
    "name": "origami/support",
    "description": "Base and helper classes for Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.co.uk"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/console": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0",
        "laravelcollective/html": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Support\\SupportServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
